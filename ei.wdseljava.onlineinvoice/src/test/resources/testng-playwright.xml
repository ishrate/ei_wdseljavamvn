<?xml version="1.0" encoding="UTF-8"?>
<suite name="Playwright Only Suite" parallel="classes" thread-count="2">
    <listeners>
        <listener class-name="com.ei.wdseljava.onlineinvoice.listeners.ExtentTestNGListener"/>
    </listeners>

    <test name="Playwright Tests">
        <description>Suite for Playwright tests</description>
        <parameter name="browser" value="chromium"/>
        <parameter name="runner" value="playwright"/>
        <classes>
            <class name="com.ei.wdseljava.onlineinvoice.tests.InvoiceEntryTest"/>
            <class name="com.ei.wdseljava.onlineinvoice.tests.GoogleSearchTest"/>
        </classes>
    </test>
</suite>